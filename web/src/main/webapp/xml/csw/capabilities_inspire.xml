            <ows:PositionName>$POS_NAME</ows:PositionName>
            <ows:ContactInfo>
                <ows:Phone>
                    <ows:Voice>00 32 2 282 47 70</ows:Voice>
                    <ows:Facsimile>00 32 2 230 31 07</ows:Facsimile>
                </ows:Phone>
                <ows:Address>
